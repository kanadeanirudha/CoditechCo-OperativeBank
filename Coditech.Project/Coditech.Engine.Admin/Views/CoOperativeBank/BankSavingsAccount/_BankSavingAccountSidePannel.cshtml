@using Coditech.Admin.Utilities;
@model Dictionary<string, string>
@{
    Layout = null;
    long bankSavingsAccountId = Convert.ToInt64(Model?.FirstOrDefault(x => x.Key == "BankSavingsAccountId").Value);
    string actionName = ViewContext.RouteData.Values["action"].ToString();
}
<div class="card sidebar-card">
    <div class="card-body">
        <div class="files-nav">
            <div class="nav flex-column nav-pills" id="files-tab" aria-orientation="vertical">
                <a class="nav-link" href="/BankSavingsAccount/Edit?bankSavingsAccountId=@bankSavingsAccountId" aria-selected="true">
                    <i class="ti-menu"></i>
                    <h5 class="m-0 ps-2">Bank Account Details</h5>
                </a>
                <a class="nav-link" href="/BankSavingsAccount/UpdateSavingsAccountClosures?bankSavingsAccountId=@bankSavingsAccountId" aria-selected="true">
                    <i class="fa fa-money-bill"></i>
                    <h5 class="m-0 ps-2">Bank Saving Account Closures</h5>
                </a>
                <a class="nav-link" href="/BankSavingsAccount/UpdateBankSavingAccountIntrestPostings?bankSavingsAccountId=@bankSavingsAccountId" aria-selected="true">
                    <i class="fas fa-receipt"></i>
                    <h5 class="m-0 ps-2">Bank Interest Posting</h5>
                </a>
                <a class="nav-link" href="/BankSavingsAccount/UpdateBankSavingsAccountTransactions?bankSavingsAccountId=@bankSavingsAccountId" aria-selected="true">
                    <i class="ti-wallet"></i>
                    <h5 class="m-0 ps-2">Savings Account Transaction</h5>
                </a>
            </div>
        </div>
    </div>
</div>